  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "mtgen"
  },
  "repository": "Bounced-Inc/mongoose-tsgen",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "npm test && rm -rf lib && tsc -b",
