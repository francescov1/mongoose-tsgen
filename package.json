    "build": "npx tsc",
    "readme": "./node_modules/@oclif/dev-cli/bin/run readme"
  },
  "types": "lib/index.d.ts",
  "lint-staged": {
    "*.{js,ts}": [
      "npx prettier --write",
      "eslint --cache --fix"
