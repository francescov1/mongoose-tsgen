    "@oclif/plugin-help": "^3.2.0",
    "cli-ux": "^5.5.0",
    "flat": "^5.0.2",
    "glob": "^7.1.6",
    "lodash": "^4.17.20",
    "mkdirp": "^1.0.4",
    "mongoose": "^5.11.13",
    "prettier": "^2.1.2",
    "strip-json-comments": "^3.1.1",
    "ts-morph": "^8.1.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/eslint": "^7.2.4",
    "@types/flat": "^5.0.1",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.167",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^10.17.35",
    "eslint": "^7.13.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.1",
    "typescript": "^3.9.7"
